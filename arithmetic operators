------------------------------------------------------------------------------------------------------------------------------------------------------

a=int(input("Enter 1st Number"))
b=int(input("Enter 2nd Number")).
add=a+b
sub=a-b
mul=a*b
div=a/b
exp=a**b
fldiv=a//b
print("Addition is=",add)
print("Subtraction=",sub)
print("Multiplication =",mul)
print("Division=",div)
print("Exponential=",exp)
print("Floor Division=",fldiv)
